<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <servlet>
        <servlet-name>RoomFinderServlet</servlet-name>
        <servlet-class>com.jivesoftware.it.RoomFinderServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LocationServlet</servlet-name>
        <servlet-class>com.jivesoftware.it.LocationServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RoomFinderServlet</servlet-name>
        <url-pattern>/room-finder</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LocationServlet</servlet-name>
        <url-pattern>/locations</url-pattern>
    </servlet-mapping>

</web-app>

